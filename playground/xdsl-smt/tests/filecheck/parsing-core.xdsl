builtin.module() {
    %true: !smt.bool = smt.constant_bool() ["value" = !smt.bool_attr<true>]
    %false: !smt.bool = smt.constant_bool() ["value" = !smt.bool_attr<false>]
    %x: !smt.bool = smt.declare_const()
    %xtrue: !smt.bool = smt.eq(%x: !smt.bool, %true: !smt.bool)
    %xfalse: !smt.bool = smt.eq(%x: !smt.bool, %false: !smt.bool)
    %xand: !smt.bool = smt.or(%xtrue: !smt.bool, %xfalse: !smt.bool)
    smt.assert(%xand: !smt.bool)
    smt.check_sat()
}