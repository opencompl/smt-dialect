// RUN: xdsl-smt.py %s | xdsl-smt.py %s -t=smt | filecheck %s
// RUN: xdsl-smt.py %s -t=smt | z3 -in

builtin.module() {
    %true: !smt.bool = smt.constant_bool true
    %false: !smt.bool = smt.constant_bool false
    %implies: !smt.bool = smt.implies %false => %true
    smt.assert(%implies : !smt.bool)
}

// CHECK: (assert (=> false true))